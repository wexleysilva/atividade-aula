programa
{
	
	funcao inicio()
	{
		real numeros[10]


		para(inteiro i=0; i < 10; i++){
			escreva("informe um numero: ")
			leia(numeros[i])
			//escreva(i," ")

		}

		para(inteiro i=10; i >=0; i--){
			
			escreva(numeros[i], " ")
			//escreva(i," ")

		}

	}
}

